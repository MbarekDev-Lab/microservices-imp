while ($true) {
    curl http://localhost:8000/sample-api
    Start-Sleep -Seconds 2
    Clear-Host
}


while ($true) {
    curl http://localhost:8000/sample-api
    Start-Sleep -Milliseconds 100
    Clear-Host
}

while ($true) {
    Clear-Host
    curl http://localhost:8000/sample-api
    Start-Sleep -Milliseconds 100
}

watch -n 0.1 curl http://localhost:8000/sample-api


